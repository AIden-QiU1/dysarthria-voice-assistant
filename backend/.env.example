# ASR 服务提供商配置
# 可选值: volcengine, alibaba
ASR_PROVIDER=volcengine

# 火山引擎 API 配置
VOLCENGINE_ACCESS_KEY_ID=your_access_key_id
VOLCENGINE_SECRET_ACCESS_KEY=your_secret_access_key
VOLCENGINE_APP_ID=your_app_id
VOLCENGINE_TOKEN=your_token

# 阿里云 API 配置
ALIBABA_APP_KEY=your_alibaba_app_key
ALIBABA_ACCESS_KEY_ID=your_alibaba_access_key_id
ALIBABA_ACCESS_KEY_SECRET=your_alibaba_access_key_secret

# 服务器配置
PORT=3001
